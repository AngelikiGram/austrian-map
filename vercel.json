{
    "version": 2,
    "builds": [
      { "src": "index.html", "use": "@vercel/static" },
      { "src": "styles.css", "use": "@vercel/static" },
      { "src": "build/*", "use": "@vercel/static" },
      { "src": "ui/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/", "dest": "/index.html" },
      { "src": "/build/(.*)", "dest": "/build/$1" },
      { "src": "/ui/(.*)", "dest": "/ui/$1" }
    ]
  }